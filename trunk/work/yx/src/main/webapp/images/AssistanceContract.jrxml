	<queryString><![CDATA[select ticket.num   as ticketNo,
       tm.type_name as ticketType,
       ticket.money as ticektMoney,
       (case ticket.is_turnover when 1 then '是'else '否' end) as ticketTurnOver,
       ticket.ticket_time as ticketTime,
       ticket.turnover_date as turnOverDate
  from yx_assistance          assistance,
       yx_assistance_ticket   ticket,
       yx_assistance_pay_info payInfo,
       yx_type_manage         tm
 where assistance.assistance_pay_id = payInfo.Id
   and assistance.assistance_ticket_id = ticket.id
   and tm.type_small = ticket.type
   and tm.type_big = 1004
  and payInfo.id = $P!{payInfoId}
union all
select null,
      null,
       null,
       null,
       null,
      null
  from dual where not exists (select 1  from yx_assistance assistance,
       yx_assistance_ticket   ticket,
       yx_assistance_pay_info payInfo,
       yx_type_manage         tm
 where assistance.assistance_pay_id = payInfo.Id
   and assistance.assistance_ticket_id = ticket.id
   and tm.type_small = ticket.type
   and tm.type_big = 1004
  and payInfo.id = $P!{payInfoId})]]></queryString>

