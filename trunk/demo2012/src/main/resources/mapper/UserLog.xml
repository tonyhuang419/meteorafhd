<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.fhdone.demo2012.dao.UserLogDao">

<!-- 	<cache type="org.mybatis.caches.ehcache.EhcacheCache"/> -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
	  
	<resultMap type="userLog" id="userLogResult">
		<id property="id" column="ID" />
		<result property="companyCd" column="COMPANY_CD" />
		<result property="userCd" column="USER_CD" />
		<result property="actionName" column="ACTION_NAME" />
		<result property="operationTime" column="OPERATION_TIME" />
		<result property="parameterInfo" column="PARAMETER_INFO" />
	</resultMap>
	
	<!-- if use cache , u should statement sql at XML-->
	<select id="selectUserLogByIdX" parameterType="Long" resultType="userLog">  
        select * from nasa2.Z_USER_LOG where id=#{id}   
    </select>
    
</mapper> 