<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="aggressiveLazyLoading" value="false" />
	</settings>

	<typeAliases>
		<typeAlias type="com.fhdone.demo2012.entity.UserLog" alias="userLog" />
		<typeAlias type="com.fhdone.demo2012.entity.DisableFunction" alias="disableFunction" />
	</typeAliases>
	<mappers>
		<mapper resource="mapper/UserLog.xml" />
		<mapper resource="mapper/DisableFunction.xml" />
	</mappers>

</configuration>  