<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fhd.test3.Address" table="address">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="addressName" column="addressName"></property>
		<property name="codeNumber" column="codeNumber"></property>
		<!-- 关联ID（多对一指向的那个ID） -->
		<many-to-one name="people" column="peopleId"  insert="false" update="false" />
	</class>
</hibernate-mapping>