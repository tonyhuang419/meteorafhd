<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.fhd.test3.People" table="people">
		<id name="id" column="peopleId" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="name" column="name" ></property>
		<set name="addresses" cascade="save-update" inverse="false">
			<key column="peopleId" not-null="true" />
			<one-to-many class="com.fhd.test3.Address"/>
		</set>
	</class>
</hibernate-mapping>