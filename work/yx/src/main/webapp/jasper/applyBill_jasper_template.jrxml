	<queryString><![CDATA[select minfo.con_id as conId,
       (select (item.con_item_id)
          from yx_con_item_minfo item
         where item.con_item_minfo_sid = p.fk_con_item_minfo_sid) as itemId,
       (select p.real_tax_bill_amount / ty.info
          from yx_type_manage ty
         where ty.type_small = p.fk_bill_type
           and ty.type_big = 1004) as billAmount,
       p.real_tax_bill_amount as taxBillAmount,
       tm.type_name as typeName,
       rownum as pid,
       (select (case when sp.stage_remark is not null then t.type_name || '(' ||sp.stage_remark||')'else t.type_name end) from 
            yx_con_maininfo_part   part,
            yx_con_item_stage      stage,
            yx_con_item_stage_plan sp,
            yx_type_manage t
 where part.contract_main_id = p.fk_con_main_sid
   and part.money_type = p.fk_bill_nature
   and stage.con_item_stage_sid = sp.contract_item_stage_id
   and sp.contract_maininfo_part_id = part.id
   and p.fk_con_i_stage_sid = stage.con_item_stage_sid
   and stage.item_stage_name = t.type_small and t.type_big = 1023 and rownum < =1 ) as stageName
  from yx_billandpro_relaion relation,
       yx_apply_bill         ab,
       yx_real_con_bc_plan   p,
       yx_type_manage        tm,
       yx_con_main_info      minfo
 where relation.fk_real_con_bcplan_sid = p.real_con_billpro_sid
   and relation.fk_apply_bill_sid = ab.apply_bill_sid
   and p.fk_con_main_sid = minfo.con_main_info_sid
   and tm.type_small = p.fk_bill_nature
   and tm.type_big = 1012
   and ab.apply_bill_sid=$P!{applyBillId}
union all
select null,null,null,null,null,null,null 
from dual where not exists (select 1 from yx_billandpro_relaion relation 
	where relation.fk_apply_bill_sid=$P!{applyBillId} )]]></queryString>

