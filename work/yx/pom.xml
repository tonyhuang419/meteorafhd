    <dependency>
      <groupId>org.jmock</groupId>
      <artifactId>jmock-legacy</artifactId>
      <version>2.5.1</version>
    </dependency>
  </dependencies>
  <properties>
    <logs_dir>C:/yingxiaoDir/logs</logs_dir>
    <temp_dir>C:/yingxiaoDir/tempDir</temp_dir>
    <upload_dir>C:/yingxiaoDir/UploadDir</upload_dir>
  </properties>
</project>